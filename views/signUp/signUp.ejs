<!--================Sign up Box Area =================-->
<section class="login_box_area section_gap">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="signup_box_img">
                    <img class="img-fluid" src="img/login.jpg" alt="">
                </div>
            </div>
            <div class="col-lg-6">
                <div class="login_form_inner">

                    <form class="row login_form" action="contact_process.php" method="post" id="contactForm" novalidate="novalidate">
                        <div class="col-md-12 form-group">
                            <h5>Họ tên</h5>
                            <input type="text" class="form-control" id="name" name="name" placeholder="" onfocus="this.placeholder = ''" onblur="this.placeholder = ''">
                        </div>
                        <div class="col-md-12 form-group">
                            <h5>Địa chỉ</h5>
                            <input type="text" class="form-control" id="address" name="address" placeholder="" onfocus="this.placeholder = ''" onblur="this.placeholder = ''">
                        </div>
                        <div class="col-md-12 form-group">
                            <h5>Số điện thoại</h5>
                            <input type="text" class="form-control" id="phone" name="phone" placeholder="" onfocus="this.placeholder = ''" onblur="this.placeholder = ''">
                        </div>
                        <div class="col-md-12 form-group">
                            <h5>Username</h5>
                            <input type="text" class="form-control" id="username" name="username" placeholder="" onfocus="this.placeholder = ''" onblur="this.placeholder = ''">
                        </div>
                        <div class="col-md-12 form-group">
                            <h5>Mật khẩu</h5>
                            <input type="text" class="form-control" id="pass" name="pass" placeholder="" onfocus="this.placeholder = ''" onblur="this.placeholder = ''">
                        </div>
                        <div class="col-md-12 form-group">
                            <h5>Nhập lại mật khẩu</h5>
                            <input type="text" class="form-control" id="re-pass" name="re-pass" placeholder="" onfocus="this.placeholder = ''" onblur="this.placeholder = ''">
                        </div>
                        <div class="col-md-12 form-group">
                            <div class="creat_account">
                                <input type="checkbox" id="f-option2" name="selector">
                                <label for="f-option2">I agree to the Term of User</label>
                            </div>
                        </div>
                        <div class="col-md-12 form-group">
                            <button type="submit" value="submit" class="primary-btn" >Sign up</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!--================End Sign up Box Area =================-->

